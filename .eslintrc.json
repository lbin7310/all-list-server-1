{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "standard",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "semi": 0,
        "no-unused-vars": 0,
        "quotes" : 0,
        "spaced-comment" : 0
    }
}